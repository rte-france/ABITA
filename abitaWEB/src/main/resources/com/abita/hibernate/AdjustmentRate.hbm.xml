<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.abita.dao.adjustmentrate.entity">
  <class name="com.abita.dao.adjustmentrate.entity.AdjustmentRateEntity" table="ADJUSTMENT_RATE">
    <id name="id" type="java.lang.Long" column="ARA_ID">
      <generator class="sequence">
        <param name="sequence">SEQ_ADJUSTMENT_RATE</param>
      </generator>
    </id>
    <property name="value" column="ARA_VALUE" type="java.math.BigDecimal" />
  </class>
</hibernate-mapping>
