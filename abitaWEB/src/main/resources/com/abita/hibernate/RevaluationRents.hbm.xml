<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.abita.dao.revaluationrents.entity">
  <class name="com.abita.dao.revaluationrents.entity.RevaluationRentsEntity" table="REVALUATION_RENTS">
    <id name="id" type="java.lang.Long" column="RER_ID">
      <generator class="sequence">
        <param name="sequence">SEQ_REVALUATION_RENTS</param>
      </generator>
    </id>
    <property name="rate" column="RER_RATE" type="java.math.BigDecimal" />
    <property name="lastUpdated" column="RER_LAST_UPDATED" type="java.util.Date" />
    <property name="action" column="RER_ACTION" type="java.lang.Boolean" />
  </class>
</hibernate-mapping>
