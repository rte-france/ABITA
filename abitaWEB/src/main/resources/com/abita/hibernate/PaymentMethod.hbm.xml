<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.abita.dao.paymentmethod.entity">
  <class name="com.abita.dao.paymentmethod.entity.PaymentMethodEntity" table="PAYMENT_METHOD">
    <id name="id" type="java.lang.Long" column="PME_ID">
      <generator class="sequence">
        <param name="sequence">SEQ_PAYMENT_METHOD</param>
      </generator>
    </id>
    <property name="label" column="PME_LABEL" type="java.lang.String" />
  </class>
</hibernate-mapping>
