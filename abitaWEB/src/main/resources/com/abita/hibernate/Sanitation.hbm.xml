<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.abita.dao.sanitation.entity">
  <class name="com.abita.dao.sanitation.entity.SanitationEntity" table="SANITATION">
    <id name="id" type="java.lang.Long" column="SAN_ID">
      <generator class="sequence">
        <param name="sequence">SEQ_SANITATION</param>
      </generator>
    </id>
    <property name="label" column="SAN_LABEL" type="java.lang.String" />
  </class>
</hibernate-mapping>
