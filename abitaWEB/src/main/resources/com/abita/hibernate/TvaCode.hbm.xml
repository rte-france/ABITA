<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.abita.dao.tvacode.entity">
  <class name="com.abita.dao.tvacode.entity.TvaCodeEntity" table="TVA_CODE">
    <id name="id" column="TVA_ID">
      <generator class="sequence">
        <param name="sequence">SEQ_TVA_CODE</param>
      </generator>
    </id>
    <property name="code" column="TVA_CODE" type="java.lang.String" />
    <property name="label" column="TVA_LABEL" type="java.lang.String" />
  </class>
</hibernate-mapping>
