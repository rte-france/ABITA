<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.abita.dao.field.entity">
  <class name="com.abita.dao.field.entity.FieldEntity" table="FIELD">
    <id name="id" type="java.lang.Long" column="FIE_ID">
      <generator class="sequence">
        <param name="sequence">SEQ_FIELD</param>
      </generator>
    </id>
    <property name="label" column="FIE_LABEL" type="java.lang.String" />
  </class>
</hibernate-mapping>
