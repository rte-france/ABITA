<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.abita.dao.chargetax.entity">
  <class name="com.abita.dao.chargetax.entity.ChargeTaxEntity" table="CHARGE_TAX">
    <id name="id" type="java.lang.Long" column="CTA_ID">
      <generator class="sequence">
        <param name="sequence">SEQ_CHARGE_TAX</param>
      </generator>
    </id>
    <property name="code" column="CTA_CODE" type="java.lang.String" />
    <property name="value" column="CTA_VALUE" type="java.math.BigDecimal" />
  </class>
</hibernate-mapping>
